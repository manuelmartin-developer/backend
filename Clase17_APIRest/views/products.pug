doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Products
    body
        h1 Products
        section
            each product in data
                article 
                    h4=product.title
                    p Precio: #{product.price}
                    img(src=product.image, height=100)
                
